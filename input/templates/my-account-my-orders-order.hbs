<!DOCTYPE html>
<html lang="en">

<head>
  {{> common/htmlhead}}
</head>

<body>
  <div class="darkbg hidden"></div>
  {{> common/header}}
  <div class="my-orders">
    <div class="container">
      <div class="my-account-title">
        <span class="my-account-title-text icon-user">{{content.pageTitle}}</span>
      </div>
      <div class="row">
        <div class="col-sm-12">
          <div class="my-orders-wrapper">
            <div class="row">
              <div class="col-sm-3">
                <div class="my-account-sidebar">
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemOne.url}}">{{content.sidebarItemOne.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemTwo.url}}">{{content.sidebarItemTwo.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemThree.url}}">{{content.sidebarItemThree.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items active">
                    <a href="{{content.sidebarItemFour.url}}">{{content.sidebarItemFour.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemFive.url}}">{{content.sidebarItemFive.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items clearfix">
                    <a href="{{content.sidebarItemSix.url}}">
                      <span><img class="sidebar-wishlist-heart hidden-sm" src="{{meta.assetsPath}}/img/heart-1.png" alt="heart icon"></span>{{content.sidebarItemSix.text}}<span class="sidebar-wishlist-quantity hidden-sm">{{content.sidebarItemSix.wishlistItems}}</span>
                    </a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemSeven.url}}">{{content.sidebarItemSeven.text}}</a>
                  </div>
                </div>
              </div>
              <div class="col-sm-9">
                <div class="my-orders-order-content">
                  <div class="row">
                    <div class="col-sm-12">
                      <div class="my-orders-title">
                        {{content.contentTitle}}
                      </div>
                    </div>
                  </div>
                  <div class="your-order-details-wrapper">
                    <div class="your-order-details-title">Your Order Details</div>
                    <div class="your-order-details">
                      <span class="your-order-details-titles">Order Number:<br>Order Date:</span>
                      <span class="your-order-details-info">01445678<br>Nov 14, 2015</span>
                    </div>
                    <button class="my-orders-print-receipt-btn">Print Receipt</button>
                  </div>
                  <div class="row">
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">Shipping Address</div>
                        <div>Mr. Bruce Wayne</div>
                        <div>100 Gotham Street</div>
                        <div>Gotham City</div>
                        <div>Region</div>
                        <div>123456</div>
                        <div>Country</div><br>
                        <div>Phone: +49 123 4567890</div>
                        <div>Email: bruce.wayne@commercetools.com</div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">Billing Address</div>
                        <div>Mr. Bruce Wayne</div>
                        <div>100 Gotham Street</div>
                        <div>Gotham City</div>
                        <div>Region</div>
                        <div>123456</div>
                        <div>Country</div><br>
                        <div>Phone: +49 123 4567890</div>
                        <div>Email: bruce.wayne@commercetools.com</div>
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">Shipping Method</div>
                        <div>DHL Standart - Delivery 3-5 Days</div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">Payment Details</div>
                        <div>Credit Card</div>
                        <div>Bruce Wayne</div>
                        <div>Visa</div>
                        <div>**** **** **** **99</div>
                        <div>09.18</div>
                      </div>
                    </div>
                  </div>
                  <div class="my-orders-order-price-summary-wrapper">
                    <div class="text-right">
                      <span class="my-orders-order-promocode-title">Promotional Code Applied:</span>
                      <span class="my-orders-order-promocode">Sunny Sale</span>
                    </div>
                    <div class="text-right">
                      <div class="my-orders-order-price-titles">
                        <span>Subtotal</span><br>
                        <span>Order Discout</span><br>
                        <span>Standart Delivery</span>
                      </div>
                      <div class="my-orders-order-price">
                        <span>200,00 €</span><br>
                        <span>0,00 €</span><br>
                        <span>10,00 €</span>
                      </div>
                      <div class="order-hr"></div>
                      <div class="my-orders-order-price-titles">
                        <span>Sales Tax</span><br>
                        <span class="order-total-big">Order Total</span>
                      </div>
                      <div class="my-orders-order-price">
                        <span>0,00 €</span><br>
                        <span class="order-total-big">210,00 €</span>
                      </div>
                    </div>
                  </div>
                  <div class="order-list-summary-titles-wrapper">
                    <div class="row">
                      <div class="col-sm-6">
                        <div>
                          Product Description
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          Quantity
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          Price
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          Total Price
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="order-detail-wrapper">
                    <div class="row">
                      <div class="col-sm-2">
                        <div>
                          <img class="img-responsive" src="{{meta.assetsPath}}/img/blue-shirt.jpg" alt="Product image">
                        </div>
                      </div>
                      <div class="col-sm-4">
                        <div>
                          <div class="order-detail-product-title">Enditatem Product Name</div>
                          <div class="order-detail-product-title">Optional Line Two</div>
                          <div class="order-sku">Sunrise SUN#123456</div>
                          <div class="order-detail-color-size-title">
                            <span>Color:</span><br>
                            <span>Size:</span>
                          </div>
                          <div class="order-detail-color-size">
                            <span>Navy Blue</span><br>
                            <span>XXL</span>
                          </div>
                          <div class="order-detail-available">Immediately available</div>
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          1
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          <span class="order-detail-discount-price">100,00 €</span>
                          <span>100,00 €</span>
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          100,00 €
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="order-print-receipt-btn-wrapper">
                    <button class="order-print-receipt-btn">Print Receipt</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {{!-- {{> catalog/wishlist}} --}}
  {{> common/footer}}
  {{> common/htmlscripts}}
</body>

</html>
